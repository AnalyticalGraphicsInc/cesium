{
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    ".idea": true,
    ".metadata": true,
    ".settings": true,
    ".externalToolBuilders": true,
    ".project": true,
    "launches": true,
    "Build": "explorerExcludedFiles",
    "Cesium-*.zip": "explorerExcludedFiles",
    "cesium-*.tgz": "explorerExcludedFiles",
    ".DS_Store": "explorerExcludedFiles",
    "Thumbs.db": "explorerExcludedFiles",
    ".eslintcache": "explorerExcludedFiles",
    "Apps/*": "explorerExcludedFiles",
    "Source/Cesium.js": "explorerExcludedFiles",
    "Specs/SpecList.js": "explorerExcludedFiles",
    "Source/Shaders/**/*.js": "explorerExcludedFiles",
    "Source/ThirdParty/Shaders/**/*.js": "explorerExcludedFiles",
    "Source/Workers/**": "explorerExcludedFiles",
    "!/Source/Workers/cesiumWorkerBootstrapper.js": "explorerExcludedFiles",
    "!/Source/Workers/transferTypedArrayTest.js": "explorerExcludedFiles",
    "node_modules": "explorerExcludedFiles",
    "npm-debug.log": "explorerExcludedFiles",
    "npm-debug.log.*": "explorerExcludedFiles",
    "package-lock.json": "explorerExcludedFiles",
    "yarn.lock": "explorerExcludedFiles",
    ".idea/workspace.xml": "explorerExcludedFiles",
    ".idea/tasks.xml": "explorerExcludedFiles",
    ".vscode/settings.json": "explorerExcludedFiles",
    ".github": "explorerExcludedFiles",
    ".concierge": "explorerExcludedFiles"
  },
  "search.exclude": {
    "Build": true,
    "Instrumented": true,
    "node_modules": true,
    "Source/Shaders/**/*.js": true
  },
  "files.associations": {
    "*.czml": "json"
  },
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "editor.insertSpaces": true,
  "editor.tabSize": 4,
  "editor.detectIndentation": false,
  "eslint.enable": true,
  "javascript.format.insertSpaceAfterCommaDelimiter": true,
  "javascript.format.insertSpaceAfterSemicolonInForStatements": true,
  "javascript.format.insertSpaceBeforeAndAfterBinaryOperators": true,
  "javascript.format.insertSpaceAfterKeywordsInControlFlowStatements": true,
  "javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": false,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": false,
  "javascript.format.placeOpenBraceOnNewLineForFunctions": false,
  "javascript.format.placeOpenBraceOnNewLineForControlBlocks": false
}